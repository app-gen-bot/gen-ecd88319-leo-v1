{
  "mcpServers": {
    "graphiti": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-graphiti"
      ],
      "env": {
        "FALKORDB_HOST": "localhost",
        "FALKORDB_PORT": "6379",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      }
    },
    "context_manager": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-context-manager"
      ],
      "env": {
        "CONTEXT_STORAGE_PATH": "/Users/labheshpatel/.cc_context_manager"
      }
    },
    "tree_sitter": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-tree-sitter"
      ]
    },
    "integration_analyzer": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-integration-analyzer"
      ]
    },
    "browser": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-browser"
      ],
      "env": {
        "BROWSER_HEADLESS": "${BROWSER_HEADLESS:-false}"
      }
    },
    "build_test": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-build-test"
      ]
    },
    "dev_server": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-dev-server"
      ]
    },
    "package_manager": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-package-manager"
      ]
    },
    "shadcn": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-shadcn"
      ]
    },
    "cwd_reporter": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-cwd-reporter"
      ]
    },
    "mem0": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "mcp-mem0"
      ],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "cc-core-password",
        "QDRANT_URL": "http://localhost:6333",
        "QDRANT_COLLECTION_NAME": "app_factory_memories",
        "MEM0_USER_ID": "app_factory",
        "MEM0_DEFAULT_CONTEXT": "app_factory"
      }
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--isolated"
      ]
    }
  }
}