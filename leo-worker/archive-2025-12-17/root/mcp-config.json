{
  "mcpServers": {
    "mem0": {
      "command": "uv",
      "args": ["run", "mcp-mem0"],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USER": "neo4j",
        "NEO4J_PASSWORD": "cc-core-password",
        "QDRANT_URL": "http://localhost:6333",
        "QDRANT_COLLECTION": "app_factory_memories",
        "MEM0_STORAGE_PATH": "${HOME}/.mem0",
        "MEM0_NAMESPACE": "orchestrator_prds",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      }
    },
    "graphiti": {
      "command": "uv",
      "args": ["run", "mcp-graphiti"],
      "env": {
        "FALKORDB_HOST": "localhost",
        "FALKORDB_PORT": "6379",
        "FALKORDB_GRAPH": "app_factory_kg",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      }
    },
    "context_manager": {
      "command": "uv",
      "args": ["run", "mcp-context-manager"],
      "env": {
        "CONTEXT_STORAGE_PATH": "${HOME}/.cc_context_manager"
      }
    },
    "tree_sitter": {
      "command": "uv",
      "args": ["run", "mcp-tree-sitter"]
    },
    "integration_analyzer": {
      "command": "uv",
      "args": ["run", "mcp-integration-analyzer"]
    },
    "browser": {
      "command": "uv",
      "args": ["run", "mcp-browser"],
      "env": {
        "BROWSER_HEADLESS": "${BROWSER_HEADLESS:-true}"
      }
    },
    "build_test": {
      "command": "uv",
      "args": ["run", "mcp-build-test"]
    },
    "cwd_reporter": {
      "command": "uv",
      "args": ["run", "mcp-cwd-reporter"]
    },
    "dev_server": {
      "command": "uv",
      "args": ["run", "mcp-dev-server"]
    },
    "package_manager": {
      "command": "uv",
      "args": ["run", "mcp-package-manager"]
    },
    "shadcn": {
      "command": "uv",
      "args": ["run", "mcp-shadcn"]
    }
  }
}